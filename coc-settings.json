{
    "python.jediEnabled": false,
    "python.venvPath": "~/.cache/pypoetry/virtualenvs",
    "list.insertMappings": {
        "<C-a>": "prompt:start",
        "<C-e>": "prompt:end",
        "<C-f>": "prompt:right",
        "<C-b>": "prompt:left",
        "<C-d>": "prompt:deletebackward",
        "<C-h>": "prompt:deleteforward",
        "<C-n>": "normal:j",
        "<C-p>": "normal:k"
    }
}
